import os
class Dog:
    
    attr1 = "mammal"
   
    
    
    def __init__(self, name):
        self.name = name
        print(" " ,os.path.basename(__file__))

    def speak(self):
        print(" my name is:",self.name)
        

Pug = Dog("Veni")
Tommy = Dog("Tommy")



Pug.speak()
Tommy.speak()